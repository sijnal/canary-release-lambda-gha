{
    {
        "Version": "2012-10-17",
        "Statement": [
            {
                "Effect": "Allow",
                "Action": [
                    "lambda:GetAlias",
                    "lambda:UpdateAlias"
                ],
                "Resource": "arn:aws:lambda:us-east-1:891377042460:function:MyLambdaFunctionCanary"
            }
        ]
    }


    {
        "Version": "2012-10-17",
        "Statement": [
            {
                "Effect": "Allow",
                "Action": [
                    "logs:DescribeLogStreams",
                    "logs:GetLogEvents"
                ],
                "Resource": [
                    "arn:aws:logs:us-east-1:891377042460:log-group:/aws/lambda/MyLambdaFunctionCanary:*"
                ]
            }
        ]
    }


    {
        "Version": "2012-10-17",
        "Statement": [
            {
                "Effect": "Allow",
                "Action": [
                    "events:DisableRule",
                    "events:DescribeRule"
                ],
                "Resource": "arn:aws:events:us-east-1:891377042460:rule/CanaryDeploymentScheduledRule"
            }
        ]
    }
}